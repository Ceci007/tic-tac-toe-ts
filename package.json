{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "typecheck": "tsc",
        "format:check": "npx prettier --check \"**/*.{js,jsx,json,ts,tsx}\"",
        "format": "npx prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run typecheck && lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": "eslint",
        "**/*.{js,jsx,json,ts,tsx}": "prettier --write"
    },
    "dependencies": {
        "@aws-amplify/cli": "^7.5.4",
        "@expo-google-fonts/delius-unicase": "^0.1.0",
        "@expo/metro-config": "^0.3.4",
        "@react-native-async-storage/async-storage": "~1.15.0",
        "@react-native-community/clipboard": "^1.5.1",
        "@react-native-community/masked-view": "0.1.10",
        "@react-native-community/netinfo": "6.0.2",
        "@react-navigation/native": "^5.9.8",
        "@react-navigation/stack": "^5.14.9",
        "@twotalltotems/react-native-otp-input": "^1.3.7",
        "aws-amplify": "^3.4.3",
        "aws-amplify-react-native": "^4.3.3",
        "expo": "^43.0.0",
        "expo-ads-admob": "~11.0.3",
        "expo-app-loading": "~1.2.1",
        "expo-av": "~10.1.3",
        "expo-font": "~10.0.3",
        "expo-haptics": "~11.0.3",
        "expo-linear-gradient": "~10.0.3",
        "expo-notifications": "~0.13.3",
        "expo-status-bar": "~1.1.0",
        "graphql-tag": "^2.11.0",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-native": "0.64.3",
        "react-native-gesture-handler": "~1.10.2",
        "react-native-modal": "^11.6.1",
        "react-native-reanimated": "~2.2.0",
        "react-native-safe-area-context": "3.3.2",
        "react-native-screens": "~3.8.0",
        "react-native-web": "~0.13.12",
        "zen-observable": "^0.8.15"
    },
    "devDependencies": {
        "@types/react": "~17.0.21",
        "@types/react-dom": "~17.0.9",
        "@types/react-native": "~0.64.12",
        "@types/zen-observable": "^0.8.2",
        "@typescript-eslint/eslint-plugin": "^4.4.0",
        "@typescript-eslint/parser": "^4.4.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^7.10.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-react": "^7.21.3",
        "husky": "^4.3.0",
        "lint-staged": "^10.4.0",
        "metro-config": "^0.66.2",
        "metro-react-native-babel-preset": "^0.66.2",
        "prettier": "2.1.2",
        "react-native-web": "0.17.1",
        "typescript": "~4.3.5"
    },
    "private": true
}
